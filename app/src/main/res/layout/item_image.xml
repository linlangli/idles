<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/item_image_imageView_one"
        android:layout_width="0dp"
        android:layout_height="100dp"
        app:layout_constraintEnd_toStartOf="@id/item_image_imageView_two"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_one_select"
        app:layout_constraintStart_toStartOf="@id/item_image_imageView_one"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_one"
        android:layout_margin="10dp"
        android:layout_width="30dp"
        android:layout_height="30dp"
        tools:ignore="ContentDescription"
        android:src="@drawable/ic_select"/>

    <ImageView
        android:id="@+id/item_image_imageView_two"
        app:layout_constraintStart_toEndOf="@id/item_image_imageView_one"
        app:layout_constraintEnd_toStartOf="@id/item_image_imageView_three"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_one"
        app:layout_constraintBottom_toBottomOf="@id/item_image_imageView_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_two_select"
        app:layout_constraintStart_toStartOf="@id/item_image_imageView_two"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_two"
        android:layout_margin="10dp"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_select"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_three"
        app:layout_constraintEnd_toStartOf="@id/item_image_imageView_four"
        app:layout_constraintStart_toEndOf="@id/item_image_imageView_two"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_one"
        app:layout_constraintBottom_toBottomOf="@id/item_image_imageView_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_three_select"
        app:layout_constraintStart_toStartOf="@id/item_image_imageView_three"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_three"
        android:layout_margin="10dp"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_select"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_four"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/item_image_imageView_three"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_one"
        app:layout_constraintBottom_toBottomOf="@id/item_image_imageView_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/item_image_imageView_four_select"
        app:layout_constraintStart_toStartOf="@id/item_image_imageView_four"
        app:layout_constraintTop_toTopOf="@id/item_image_imageView_four"
        android:layout_margin="10dp"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_select"
        tools:ignore="ContentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>